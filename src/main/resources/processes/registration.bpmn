        <di:waypoint x="430" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_0icmd0y_di" bpmnElement="Flow_0icmd0y">
        <di:waypoint x="215" y="117" />
        <di:waypoint x="280" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_0tzq1li_di" bpmnElement="Flow_0tzq1li">
        <di:waypoint x="820" y="117" />
        <di:waypoint x="902" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNShape id="_BPMNShape_StartEvent_2" bpmnElement="registrationStartEvent">
        <dc:Bounds x="179" y="99" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_12hvsd1_di" bpmnElement="enterAccountData">
        <dc:Bounds x="280" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_1ke79vi_di" bpmnElement="createUser">
        <dc:Bounds x="430" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_1fmyb7x_di" bpmnElement="confirmAccount">
        <dc:Bounds x="570" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_1a1teiy_di" bpmnElement="Event_1a1teiy">
        <dc:Bounds x="902" y="99" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_0nb6dss_di" bpmnElement="fileUpload">
        <dc:Bounds x="720" y="77" width="100" height="80" />
